<h1>You've added a new car listing!</h1>
<ul>
  @foreach(var car in Model)
  {
    <li>@car.GetMakeModel()</li>
    <li>"$" + @car.GetPrice()</li>
    <li>"Mileage: " + @car.GetMiles()</li>
    <li>@car.GetDetails()</li>
  }
</ul>

<p><a href="/cars/new">Add another Car</a></p>
<p><a href="/cars">See all listings</a></p>
